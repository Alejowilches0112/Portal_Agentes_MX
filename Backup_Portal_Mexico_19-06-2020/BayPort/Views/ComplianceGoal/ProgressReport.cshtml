                                                <td>{{item.InitialRange| currency}} </td>
                                                <td>{{item.FinalRange  | currency}}</td>
                                                <td>{{item.CategoryName }} </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
