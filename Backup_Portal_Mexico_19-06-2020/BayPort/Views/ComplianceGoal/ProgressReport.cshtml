                                                <td align="center">{{item.LoanCount}}</td>
                                                <td align="center">{{item.LoanAmount| currency}} </td>
                                                <td align="center">{{item.Average| currency}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>


