                                                <table class="table table-hover" style="margin-bottom: 0px">
                                                    <thead>
                                                        <tr style="font-size: small; text-align: center;">
                                                            <th align="center"></th>
                                                            <th align="center">#CREDITOS</th>
                                                            <th align="center">VALOR DESEMBOLSO</th>
                                                            <th align="center">VALOR CREDITO</th>
                                                        </tr>
                                                    </thead>
