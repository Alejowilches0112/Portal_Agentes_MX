                                                            <td align="center">{{Progress.progressExecutive.DisbursementAmount| currency}} </td>
                                                            <td align="center">{{Progress.progressExecutive.LoanValue| currency}}</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
