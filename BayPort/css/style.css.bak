/*========================================================
                     MEDIA QUERIES
=========================================================*/
@media (min-width: 1220px) {
  .container {
    padding: 0;
  }
}
@media (max-width: 767px) {
  .row .col-xs-12 {
    margin-top: 30px;
    margin-bottom: 30px;
  }
}
@media (max-width: 991px) {
  .row .col-sm-12 {
    margin-top: 30px;
    margin-bottom: 30px;
  }
}
@media (max-width: 991px) {
  .row .col-sm-6 {
    margin-top: 30px;
    margin-bottom: 30px;
  }
}
@media (max-width: 479px) {
  .row .col-xs-4 {
    float: none;
    margin: 15px auto;
    width: 100%;
  }
}

@media (max-width: 479px) {
  .panel-xs {
    float: none;
    margin: 15px auto;
    width: 85% !important;
  }
}

